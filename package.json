{
  "name": "Bootstrps",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "scss": "node-sass -o styles/ styles/",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "watch": "onchange 'styles/*.scss' -- npm run scss",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin prices.html -d dist --htmlmin -o dist/prices.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "bootstrap": "^5.0.0-beta1",
    "bootstrap-icons": "^1.3.0",
    "jquery": "^3.5.1",
    "lite-server": "^2.6.1",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "imagemin-cli": "^6.0.0",
    "uglify": "^0.1.5",
    "usemin-cli": "^0.6.0"
  }
}
